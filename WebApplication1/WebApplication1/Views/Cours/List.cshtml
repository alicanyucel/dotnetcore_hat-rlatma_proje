@{
}
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title></title>
</head>
<body>
    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Phone</th>
            </tr>
        </thead>
        @foreach (var item in Model)
        {
    <tr>
        <td><p> @item.Name </p></td>
        <td><p> @item.Email </p></td>
        <td><p> @item.Phone </p></td>
    </tr>
        }
    </table>
</body>
</html>
